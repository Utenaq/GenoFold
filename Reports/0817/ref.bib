journal = {Nucleic Acids Research},
month = {jan},
number = {D1},
pages = {D259--D265},
pmid = {26464443},
title = {{RMBase: A resource for decoding the landscape of RNA modifications from high-throughput sequencing data}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26464443 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4702777},
volume = {44},
year = {2016}
}
