keywords = {DNA,RNA,base pair,convexity,dilute solution,distinguishability,duality AMS subject classifications,dynamic programming,equilibrium,minimum free energy,multiple strands,overcounting,partition function,redundancy,secondary structure,sym-metry},
number = {1},
pages = {65--88},
pmid = {244514000006},
title = {{Thermodynamic Analysis of Interacting Nucleic Acid Strands}},
