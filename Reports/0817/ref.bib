isbn = {0066-426X (Print)$\backslash$r0066-426X (Linking)},
issn = {0066-426X},
journal = {Annual Review of Physical Chemistry},
keywords = {,confined fluids,hydrophobic effect,nanofluidics,nanopores,nanoscale drying,nanoscale drying transitions,protein hydration,proton transfer,transitions},
month = {may},
number = {1},
pages = {713--740},
pmid = {18092942},
