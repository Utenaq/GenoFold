@techreport{Thirumalai2001,
abstract = {We describe a conceptual framework for understanding the way large RNA molecules fold based on the notion that their free-energy landscape is rugged. A key prediction of our theory is that RNA folding can be described by the kinetic partitioning mechanism (KPM). According to KPM a small fraction of molecules folds rapidly to the native state whereas the remaining fraction is kinetically trapped in a low free-energy non-native state. This model provides a unified description of the way RNA and proteins fold. Single-molecule experiments on Tetrahymena ribozyme, which directly validate our theory, are analyzed using KPM. We also describe the earliest events that occur on microsecond time scales in RNA folding. These must involve collapse of RNA molecules that are mediated by counterion-condensation. Estimates of time scales for the initial events in RNA folding are provided for the Tetrahymena ribozyme.},
author = {Thirumalai, D and Lee, Namkyung and Woodson, Sarah A and Klimov, D K},
booktitle = {Annu.Rev.Phys.Chem.},
file = {:home/utena/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Thirumalai et al. - 2001 - EARLY EVENTS IN RNA FOLDING.pdf:pdf},
keywords = {,Kinetic partitioning mechanism,RNA folding pathways,Tetrahymena ribozyme,counterion-condensation s,specific and nonspecific collapse},
number = {0066-426X},
pages = {751--762},
pmid = {11326079},
title = {{Early Events in Rna Folding}},
url = {www.annualreviews.org},
volume = {52},
year = {2001}
}
@article{Yang2015,
abstract = {Despite great advances in molecular dynamics simulations, there remain large gaps between the simulations and experimental observations in terms of the time and length scales that can be approached. Developing fast and accurate algorithms and methods is of ultimate importance to bridge these gaps. In this Account, we briefly summarize recent efforts in such directions. In particular, we focus on integrated tempering sampling. The efficiency of this sampling method has been demonstrated by applications to a range of chemical and biological problems: protein folding, molecular cluster structure searches, and chemical reactions. The combination of integrated tempering sampling and a trajectory sampling method allows the calculation of rate constants and reaction pathways without predefined collective coordinates.},
author = {Yang, Lijiang and Liu, Cheng En and Shao, Qiang and Zhang, Jun and Gao, Yi Qin},
doi = {10.1021/ar500267n},
file = {:home/utena/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yang et al. - 2015 - From Thermodynamics to Kinetics Enhanced Sampling of Rare Events(2).pdf:pdf;:home/utena/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yang et al. - Unknown - From Thermodynamics to Kinetics Enhanced Sampling of Rare Events(2).pdf:pdf},
