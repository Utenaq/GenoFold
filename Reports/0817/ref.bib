@techreport{Gultyaev1995,
abstract = {A procedure for simulating the RNA folding process using the principles of genetic algorithm is proposed. The method allows one to simulate a folding pathway of RNA, including such processes as disruption of temporarily formed structures, the folding of a molecule during its synthesis and pseudoknot formation. The simulations are able to predict functional metastable foldings and kinetically driven transitions to more stable structures. The analysis of free energies for intermediate foldings allows estimation of the ranges of kinetic refolding barriers and suggests that in some RNAs the selective evolutionary pressure suppresses the possibilities for alternative structures that could form in the course of transcription. It is shown that the folding pathway simulation can result in structure predictions that are more consistent with phylogenetically proven structures than minimum energy solutions. This suggest that RNA folding kinetics is very important for the formation of functional RNA structures. Therefore, apart form its value for predictions of RNA structures, the proposed computer simulations can be a powerful tool in the studies of RNA folding features. {\textcopyright} 1995 Academic Press Limited.},
author = {Gultyaev, Alexander P. and {Van Batenburg}, F. H.D. and Pleij, Cornelis W.A.},
